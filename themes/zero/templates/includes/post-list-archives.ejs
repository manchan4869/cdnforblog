<div class="autopagerize_page_element">
  <div class="content">
    <div class="list_with_title">
      <div class="listing_title">标签</div>
      <div class="tags-container">
        <%site.tags.forEach((tag)=>{%>
          <a class="tag" href="<%= tag.link %>">
            <%=tag.name%></a>
          <%});%></div>
    </div>
    <%let years=[];posts.forEach((item)=>{const year=item.date.substring(0,4);if(!years.includes(year)){years.push(year)}});%>
      <%years.forEach(function(year){%>
        <div class="list_with_title">
          <div class="listing_title">
            <%-year%></div>
          <div class="listing">
            <div class="listing-item">
              <%posts.forEach(function(post){%>
                <%if(post.date.indexOf(year)!==-1){%>
                  <div class="listing_post">
                    <a href="<%= post.link %>" title="<%= post.title %>">
                      <%=post.title%></a>
                    <div class="post_time">
                      <span class="date">
                        <%=site.utils.moment(post.date).format( "MM.DD")%></span>
                    </div>
                  </div>
                  <%}%>
                    <%});%></div>
          </div>
        </div>
        <%});%></div>
  <div class="paginator pager pagination no_pages">
    <div class="paginator_container pagination_container"></div>
  </div>
</div>